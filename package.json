{
  "name": "vue-burger",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build": "vue-cli-service build ./src/index.js",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:example": "vue-cli-service build ./example/main.js --dest ./example/dist",
    "prepare": "npm run build",
    "publish": "npx np"
  },
  "files": [
    "dist/css",
    "dist/js"
  ],
  "dependencies": {
    "hamburgers": "^1.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "next",
    "@vue/cli-plugin-eslint": "next",
    "@vue/cli-plugin-unit-jest": "^4.0.0-rc.7",
    "@vue/cli-service": "next",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": ">= 2.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
