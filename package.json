{
  "name": "vue-burgers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build": "vue-cli-service build ./src/index.js",
    "build:example": "vue-cli-service build ./example/main.js --dest ./example/dist",
    "lint": "vue-cli-service lint",
    "prepare": "npm run build",
    "publish": "npx np"
  },
  "dependencies": {
    "hamburgers": "^1.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "next",
    "@vue/cli-plugin-eslint": "next",
    "@vue/cli-service": "next",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": ">= 2.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/css",
    "dist/js"
  ]
}
